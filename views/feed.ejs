	<div class="row">

		<!-----Facebook----->
		<div class="col-sm-3">
			<% if (facebook_data != null) { %>
			<div class="chat-panel panel panel-primary">
				<div class="panel-heading">
					<i class="fa fa-facebook-square fa-lg" aria-hidden="true"></i> Facebook Feed
				</div>
				<div class="panel-body">
					<ul class="chat">
						<% for(var i = 0; i < facebook_data.length; i++) { %>
						<li class="left clearfix">
							<div class="chat-body clearfix">
								<p>
									<%= facebook_data[i].message %>
								</p>
								<p>
									<%= facebook_data[i].story %>
								</p>
							</div>
						</li>
						<% };; %>
					</ul>
				</div>
			</div>
			<% } %>
		</div>

		<!-----Twitter----->
		<div class="col-sm-5">
			<% if (twitter_data != null) { %>
			<div class="chat-panel panel panel-info">
				<div class="panel-heading">
					<i class="fa fa-twitter-square fa-lg" aria-hidden="true"></i> Twitter Feed
				</div>

				<div class="panel-body">
					<ul class="chat">
						<% for(var i = 0; i < twitter_data.length; i++) { %>
						<li class="left clearfix">
							<div class="chat-body clearfix">
								<p>
									<%= twitter_data[i].text %> <br><br>
									<b>Posted by: </b><%= twitter_data[i].user.name %>
								</p>
							</div>
						</li>
						<% };; %>
					</ul>
				</div>
			</div>

			<% } %>
		</div>

